.sprite {
    background-image: url(spritesheet.png);
    background-repeat: no-repeat;
    display: block;
}

.sprite-_ {
    width: 25px;
    height: 25px;
    background-position: -5px -5px;
}

.sprite-_-black {
    width: 29px;
    height: 29px;
    background-position: -40px -5px;
}

.sprite-_-gray {
    width: 20px;
    height: 19px;
    background-position: -79px -5px;
}

.sprite-_-orange {
    width: 20px;
    height: 21px;
    background-position: -79px -34px;
}

